@startuml diagrama_de_objetos

object carro
carro : marca = "FIAT"
carro : modelo = "Uno"
carro : ano = "2008"
carro : placa = "DANCE"
carro : quilometragem = 450
carro : valorDaDiaria = 80

carro : + set_estado(self) carro

object carro2
carro2 : marca = "FORD"
carro2 : modelo = "4 Rodas"
carro2 : ano = "2019"
carro2 : placa = "GODS"
