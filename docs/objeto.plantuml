aluguel2 : diaria = carro.valorDaDiaria
aluguel2 : qtDays = hj - dataFim
aluguel2 : taxa = qtDays * diaria * (20/100)
aluguel2 : nomeClasse = "Aluguel"

aluguel "1" *-- carro
aluguel "1" *-- cliente
aluguel2 "1" *-- carro2
